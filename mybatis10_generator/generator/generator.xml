<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- 打开命令口(win+R),在命令窗口中进入当前文件所在目录，执行下面的命令 -->
<!-- java -jar mybatis-generator-core-1.3.2.jar -configfile generator.xml -overwrite-->
<generatorConfiguration>
	<!-- 驱动jar包，mssql使用jtds-1.2.jar,mysql使用mysql-connector-java-5.0.8-bin.jar -->
	<classPathEntry location="./mysql-connector-java-commercial-5.1.25-bin.jar" />
	<context id="FreeCMS_Tables" targetRuntime="MyBatis3">
		<!-- mysql配置 -->
		<!-- 改1:  数据名和密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/j19051"
			userId="root" 
			password="root">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- javabean生成文件的存放位置 -->
		<!-- 改2: javabean所在的包,发果包不存在，会被自动创建 -->  
		<javaModelGenerator targetPackage="com.fs.po"
			targetProject="../src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- mybatis映射文件的存放位置 -->
		<!-- 改3: 映射文件生成时所在的目录，如果该目录不存在，会被自动创建,  mybatis_mapping -->
		<sqlMapGenerator targetPackage="mybatis_mapping" targetProject="../src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- Mapper接口文件的存放位置 -->
		<!--改4  mapper接口所在的包,如果包不存在，会被自动创建 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.fs.mapper" targetProject="../src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 要生成的表 -->
		<!-- 改5  表名和javabean的类名 -->
		<table tableName="goods" domainObjectName="Goods"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" />
		<table tableName="record" domainObjectName="Record"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" />
		<table tableName="stu" domainObjectName="Stu"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" />
		<table tableName="type" domainObjectName="Type"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" />
		<table tableName="user" domainObjectName="User"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" />
	</context>
</generatorConfiguration>